packager="Yaolinux Team"
maintainer="sipo"

description="ISO AAC audio decoder"
url="http://faac.sourceforge.net/"

name=faad2
version=2.10.0

source=(https://github.com/knik0/faad2/archive/${version//./_}.tar.gz)

prepare() {

mv -v "${name}-${version//./_}" "${name}-${version}"

  cd ${name}-${version}

  autoreconf -fiv
}

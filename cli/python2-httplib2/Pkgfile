makedepends=(python2-setuptools)

description="Comprehensive HTTP client library, supporting many features."
url="https://pypi.python.org/pypi/httplib2"

contributors="tnut"
packager="greg<greg@nutyx.org>"

name=python2-httplib2
version=0.13.1

source=(https://files.pythonhosted.org/packages/ce/ed/803905d670b52fa0edfdd135337e545b4496c2ab3a222f1449b7256eb99f/httplib2-$version.tar.gz)

build() {

    cd httplib2-$version
    python2 setup.py install --prefix=/usr --root=$PKG
}

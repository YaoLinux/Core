# Depends on: flex bison


packager="sipo"
contributors=""
maintainer="sipo"


description="ACPI tools, including Intel ACPI Source Language compiler"
url="https://acpica.org"


name=acpica
version=20190215
release=1


source=(https://github.com/acpica/acpica/archive/R02_15_19.tar.gz)


build(){
  cd $name-R02_15_19
  make
}

package() {
  cd $name-R02_15_19
  make DESTDIR=${PKG} install
}
